{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Backend Server",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/server.js",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Debug Yahoo Finance Incremental",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/yahoo-finance-incremental.js",
            "args": ["SNOW", "--force"],
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Debug Symbol Search",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/symbol-search-enhanced.js",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Debug Current File",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}